# prod.env.example
# Example environment variable definitions for production
# Do not commit real secrets; use secret management for sensitive values.

REACT_APP_API_BASE_URL=https://api.airlinefareoptimization.com

# PSS Integration
PSS_API_ENDPOINT=https://api.amadeuspss.com
PSS_API_KEY=your_prod_pss_api_key

# PROS Integration
PROS_API_ENDPOINT=https://api.prosod.com
PROS_API_KEY=your_prod_pros_api_key

# JWT and authentication
JWT_SECRET=your_prod_jwt_secret
TOKEN_EXPIRY_SECONDS=3600

# Logging level
LOG_LEVEL=INFO
