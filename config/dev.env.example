# dev.env.example
# Example environment variable definitions for development

# Service URLs and keys
REACT_APP_API_BASE_URL=http://localhost:5000

# PSS Integration
PSS_API_ENDPOINT=https://api.amadeuspss.com
PSS_API_KEY=your_dev_pss_api_key

# PROS Integration
PROS_API_ENDPOINT=https://api.prosod.com
PROS_API_KEY=your_dev_pros_api_key

# JWT and authentication
JWT_SECRET=your_dev_jwt_secret
TOKEN_EXPIRY_SECONDS=3600

# Logging level
LOG_LEVEL=DEBUG
